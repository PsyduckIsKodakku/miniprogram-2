@import "../../style/weui.wxss";

page {
  line-height: unset;
  font-family: -apple-system-font, "Helvetica Neue", sans-serif;
}

image {
  display: flex;
  overflow: visible;
}

.top {
  text-align: center;
}

.second {
  margin-top: 20rpx;
  margin-bottom: 50rpx;
  text-align: center;
}

.top text {
  font-size: 50rpx;
  color: #cd5c5c;
}

.gogogo text {
  font-size: 30rpx;
  color: #333;
}

.rankingImg {
  position: absolute;
  margin-top: 8%;
  margin-left: 20%;
  width: 120rpx;
  height: 120rpx;
}

.ranking {
  position: absolute;
  text-align: center;
  font-weight: 600;
  /* font-size: 1200%; */
  margin-left: 24.5%;
  margin-top: 10%;
}
.ranking text {
  font-size: 235%;
  color: #333;
}

.info {
  display: flex;
  flex-direction: column;
  margin-left: 60%;
}

.info text {
  color: #333;
}

.damage {
  align-items: center;
  vertical-align: center;
}

.damage text {
  margin-right: 10rpx;
}

.total {
  font-weight: 500;
}

.line {
  width: 100%;
  height: 1px;
  background-color: #333;
  vertical-align: middle;
}